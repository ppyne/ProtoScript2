import Io;
import RegExp;

function main() : void {
    RegExp r = RegExp.compile("(\\w+)-(\\w+)", "");
    RegExpMatch m = r.find("alpha-beta", 0);
    Io.print(m.groups[0].concat("|").concat(m.groups[1]).concat("|").concat(m.groups[2]).concat("|").concat(m.start.toString()).concat("|").concat(m.end.toString()));
}
