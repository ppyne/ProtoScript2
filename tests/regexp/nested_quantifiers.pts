// Test goal: accept nested quantifiers on a long input and keep runtime deterministic.
// Importance: guards against regressions where valid patterns are rejected or become unstable.
import RegExp;
import Io;

function main() : void {
    RegExp r = RegExp.compile("(a+)+", "");
    string s = "";
    for (int i = 0; i < 2000; i++) {
        s = s.concat("a");
    }
    Io.printLine(r.test(s, 0));
}
