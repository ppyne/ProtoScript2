import Io;
import RegExp;

function main() : void {
    RegExp d = RegExp.compile("^\\d+$", "");
    Io.printLine(d.test("123", 0).toString());
    Io.printLine(d.test("12a", 0).toString());

    RegExp nd = RegExp.compile("^\\D+$", "");
    Io.printLine(nd.test("abc_", 0).toString());
    Io.printLine(nd.test("ab1", 0).toString());

    RegExp w = RegExp.compile("^\\w+$", "");
    Io.printLine(w.test("Ab9_", 0).toString());
    Io.printLine(w.test("ab-", 0).toString());

    RegExp nw = RegExp.compile("^\\W+$", "");
    Io.printLine(nw.test("-+!", 0).toString());
    Io.printLine(nw.test("_", 0).toString());

    RegExp s = RegExp.compile("^\\s+$", "");
    Io.printLine(s.test(" \t\r\n\f\u000B", 0).toString());
    Io.printLine(s.test(" a", 0).toString());

    RegExp ns = RegExp.compile("^\\S+$", "");
    Io.printLine(ns.test("abc_1", 0).toString());
    Io.printLine(ns.test("ab c", 0).toString());
}
