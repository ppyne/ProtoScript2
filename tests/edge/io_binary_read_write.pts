import Io;

function main() : void {
    var f = Io.open("io_bin.bin", "wb");
    var data = [1, 2, 255];
    f.write(data);
    f.close();

    var r = Io.open("io_bin.bin", "rb");
    var chunk = r.read(3);
    var tail = r.read(1);
    r.close();

    bool ok = (chunk.length() == 3) && (tail.length() == 0);
    Io.printLine(ok.toString());
}
