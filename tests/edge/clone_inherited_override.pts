import Io;

prototype MyType {
    function clone() : MyType {
        Exception e = Exception.clone();
        e.message = "forbidden";
        throw e;
    }
}

prototype MyNewType : MyType {
}

function main() : void {
    try {
        MyType mt = MyNewType.clone();
        Io.printLine("unexpected");
    } catch (Exception e) {
        Io.printLine(e.message);
    }
}
