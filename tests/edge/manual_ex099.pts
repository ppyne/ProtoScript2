import Fs;
import Io;

function main() : void {
    string base = Io.tempPath();
    Fs.mkdir(base);
    TextFile a = Io.openText(base.concat("/a.txt"), "w");
    a.write("x");
    a.close();

    Dir d = Fs.openDir(base);
    int n = 0;
    while (d.hasNext()) {
        string name = d.next();
        if (name != "") n = n + 1;
    }
    d.close();
    Fs.rm(base.concat("/a.txt"));
    Fs.rmdir(base);
    Io.printLine((n >= 1).toString());
}
