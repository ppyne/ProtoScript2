import Sys;
import Io;

function main() : void {
    list<string> args = [];
    list<byte> input = [ (byte)0xC3, (byte)0x28 ];
    ProcessResult r = Sys.execute("/bin/cat", args, input, true, false);
    ProcessEvent e = r.events()[0];
    try {
        e.data().toUtf8String();
    } catch (RuntimeException ex) {
        Io.printLine("RuntimeException");
    }
}
